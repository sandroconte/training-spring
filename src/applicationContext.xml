<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="myFortune" class="training.demo.services.implementations.HappyFortune"></bean>
	
	<bean id="myCoach" class="training.demo.models.TrackCoach">
		<constructor-arg ref="myFortune" />
	</bean>
	
	<bean id="myCricketCoach" class="training.demo.models.CricketCoach">
		<property name="fortune" ref="myFortune"></property>
	</bean>
</beans>
